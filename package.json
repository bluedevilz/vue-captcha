{
    "name": "vue-captcha",
    "version": "0.1.1",
    "description": "Input capthca component to display currency value based on Vue.",
    "author": "Hersa Arnerangi",
    "main": "dist/vue-captcha.min.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bluedevilz/vue-captcha.git"
    },
    "bugs": {
        "url": "https://github.com/bluedevilz/vue-captcha/issues"
    },
    "keywords": [
        "component",
        "botdetect",
        "captcha",
        "standalone",
        "vue",
        "vue.js"
    ],
    "homepage": "https://github.com/bluedevilz/vue-captcha",
    "license": "MIT",
    "dependencies": {},
    "scripts": {
        "test": "cross-env BABEL_ENV=test ./node_modules/.bin/karma start test/karma.config.js",
        "test:watch": "cross-env BABEL_ENV=test ./node_modules/.bin/karma start test/karma.config.js --single-run=false",
        "lint": "./node_modules/.bin/eslint --ext .js,.vue src spec",
        "build": "./node_modules/.bin/webpack --hide-modules -p --progress",
        "report-coverage": "codecov"
    },
    "devDependencies": {
        "auto-changelog": "~2.2.1",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-istanbul": "^4.1.4",
        "babel-preset-env": "^1.7.0",
        "clean-webpack-plugin": "^1.0.1",
        "css-loader": "^6.5.1",
        "npm-run-all": "~4.1.5",
        "vue": "^2.4.2",
        "vue-loader": "^13.0.4",
        "vue-template-compiler": "~2.6.12",
        "webpack": "^3.5.5"
    },
    "babel": {
        "presets": [
            [
                "env",
                {
                    "uglify": true,
                    "modules": false,
                    "targets": {
                        "browsers": [
                            "> 1%",
                            "last 2 versions",
                            "not ie <= 8"
                        ]
                    }
                }
            ]
        ],
        "env": {
            "test": {
                "plugins": [
                    "istanbul"
                ]
            }
        }
    },
    "eslintConfig": {
        "extends": [
            "eslint:recommended",
            "plugin:vue/recommended"
        ]
    }
}
