{
  "name": "vue-captcha",
  "version": "0.1.2",
  "description": "Input capthca component to display currency value based on Vue.",
  "author": "Hersa Arnerangi",
  "main": "dist/vue-captcha.umd.js",
  "module": "dist/vue-captcha.esm.js",
  "unpkg": "dist/vue-captcha.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluedevilz/vue-captcha.git"
  },
  "bugs": {
    "url": "https://github.com/bluedevilz/vue-captcha/issues"
  },
  "keywords": [
    "component",
    "botdetect",
    "captcha",
    "standalone",
    "vue",
    "vue.js"
  ],
  "homepage": "https://github.com/bluedevilz/vue-captcha",
  "license": "MIT",
  "dependencies": {
    "acorn": "^6.4.2"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-captcha.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-captcha.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-captcha.min.js"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "postcss": "^8.4.4",
    "rollup": "^1.17.0",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "uglify": true,
          "modules": false,
          "targets": {
            "browsers": [
              "> 1%",
              "last 2 versions",
              "not ie <= 8"
            ]
          }
        }
      ]
    ]
  }
}
